<!DOCTYPE html>
<html>
<head>
  <title>BandPractice</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <% if current_user %>
      Signed in as <%= current_user.username %> | <%= link_to "Sign Out", sign_out_path %>
    <% else %>
      <%= link_to "Sign Up", new_user_path %>
    <% end %>
  </header>

<%= yield %>

</body>
</html>
